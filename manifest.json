{
    "manifest_version": 3,
    "name": "Cams Notifications",
    "description": "Add the reading time to Chrome Extension documentation articles",
    "version": "1.0",
    "action":{
        "default_popup":"src/onboard.html"
    },

    "background": {
        "service_worker": "assets/background-81003b8e.js",
        "type": "module"
      },
      "host_permissions": [
        "https://cams-endpoint.onrender.com/*",
        "http://*/*", "https://*/*"

     ],
   
      "permissions": ["gcm", "storage", "notifications"]

}